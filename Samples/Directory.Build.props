<Project TreatAsLocalProperty="_SassCompilerTFM">

  <PropertyGroup>
    <SassCompilerAssembly Condition=" '$(Configuration)' != '' ">$(MSBuildThisFileDirectory)..\AspNetCore.SassCompiler\bin\$(Configuration)\netcoreapp3.1\AspNetCore.SassCompiler.dll</SassCompilerAssembly>
    <SassCompilerAssembly Condition=" '$(Configuration)' == '' ">$(MSBuildThisFileDirectory)..\AspNetCore.SassCompiler\bin\Debug\netcoreapp3.1\AspNetCore.SassCompiler.dll</SassCompilerAssembly>
  </PropertyGroup>

</Project>
